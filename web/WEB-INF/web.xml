<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <servlet>
        <servlet-name>LoadAll</servlet-name>
        <servlet-class>java.controller.LoadAllController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadAll</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateEmployee</servlet-name>
        <servlet-class>java.controller.CreateController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateEmployee</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>RegisterFilter</filter-name>
        <filter-class>java.filter.Myfilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>RegisterFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>